datasource db {
  provider = "sqlite" 
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Submission {
  id          String   @id @default(uuid())
  createdAt   DateTime @default(now())
  name        String
  nachname    String
  email       String
  newsletter  Boolean
  betaProgram Boolean
}
